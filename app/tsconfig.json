{
  "compilerOptions": {
      "allowJs": true,
      "declaration": false,
      "esModuleInterop": true,
      "incremental": true,
      "module": "commonjs",
      "moduleResolution": "node",
      "outDir": "./dist",
      "resolveJsonModule": true,
      "skipLibCheck": true,
      "sourceMap": true,
      "target": "es2017",
      "lib": ["es2017", "dom"]
  },
  "include": [
      "./src/**/*"
  ]
}
